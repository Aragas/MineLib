<Window
    x:Class="Aragas.QServer.GUI.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Aragas.QServer.GUI.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vw="clr-namespace:Aragas.QServer.GUI.Views"
    Title="NATS Explorer"
    Height="450"
    Width="800"
    DataContext="{Binding Source={StaticResource Locator}, Path=MainViewModel}"
    mc:Ignorable="d">
    <Grid>
        <TabControl>
            <TabItem Header="Сервисы">
                <vw:ServicesView Width="Auto" Height="Auto" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
