<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>PokeD.Core</RootNamespace>
    <AssemblyName>PokeD.Core</AssemblyName>
    <LangVersion>8.0</LangVersion>
	  <SolutionDir Condition="$(SolutionDir) == '' Or $(SolutionDir) == '*Undefined*'">..\..\</SolutionDir>
    <NoWarn>RCS1057;RCS1037;RCS1036;RCS1003</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PokeApi-NJ.NET" Version="1.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="$(SolutionDir)\PokeD\Libraries\PokeD.BattleEngine\PokeD.BattleEngine.csproj" />
    <ProjectReference Include="$(SolutionDir)\Third Party\Aragas.Network-2\src\Aragas.Network-2.Standard\Aragas.Network-2.csproj" />
  </ItemGroup>

</Project>
